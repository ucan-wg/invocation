type Receipt {
  s Signature
  p SignaturePayload
}

type SignaturePayload {
  h VarsigHeader
  r ReceiptPayload
}

type ReceiptPayload struct {
  iss DID
  ran &Invocation

  enq [&Task]
  out Result

  iat Integer
  mta {String : Any}

  prf [&Delegation]
  rec &Receipt
}

type Result union {
  | any "ok"
  | any "error"
} representation keyed
